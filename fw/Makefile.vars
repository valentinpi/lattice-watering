IP_PREFIX ?= 2001:db8:1::
SUBNET_IP_PREFIX ?= 2001:db8:2::
HOST_IP_ADDR ?= $(IP_PREFIX)
BR_ETHERNET_IP_ADDR ?= $(IP_PREFIX)1
BR_IEEE802154_IP_ADDR ?= $(SUBNET_IP_PREFIX)
CFLAGS += '-DIP_PREFIX="$(IP_PREFIX)"' '-DSUBNET_IP_PREFIX="$(SUBNET_IP_PREFIX)"' '-DHOST_IP_ADDR="$(HOST_IP_ADDR)"' '-DBR_ETHERNET_IP_ADDR="$(BR_ETHERNET_IP_ADDR)"' '-DBR_IEEE802154_IP_ADDR="$(BR_IEEE802154_IP_ADDR)"'
